"use strict";                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
sap.ui.define(["sap/ui/core/mvc/Controller"], function (Controller) {                                                                                                                                                                                          
  /**                                                                                                                                                                                                                                                          
   * Main View Controller                                                                                                                                                                                                                                      
   *                                                                                                                                                                                                                                                           
   * @extends sap.ui.core.mvc.Controller                                                                                                                                                                                                                       
   * @alias devepos.i18ncheck.controller.BaseController                                                                                                                                                                                                        
   * @public                                                                                                                                                                                                                                                   
   */                                                                                                                                                                                                                                                          
  const BaseController = Controller.extend("devepos.i18ncheck.controller.BaseController", {                                                                                                                                                                    
    onInit: function _onInit() {                                                                                                                                                                                                                               
      this.oRouter = this.getOwnerComponent().getRouter();                                                                                                                                                                                                     
      this.oLayoutModel = this.getOwnerComponent().getLayoutModel();                                                                                                                                                                                           
    },                                                                                                                                                                                                                                                         
    getRouter: function _getRouter() {                                                                                                                                                                                                                         
      return this.getOwnerComponent().getRouter();                                                                                                                                                                                                             
    },                                                                                                                                                                                                                                                         
    getFlexColHelper: function _getFlexColHelper() {                                                                                                                                                                                                           
      return this.getOwnerComponent().getHelper();                                                                                                                                                                                                             
    }                                                                                                                                                                                                                                                          
  });                                                                                                                                                                                                                                                          
  return BaseController;                                                                                                                                                                                                                                       
});                                                                                                                                                                                                                                                            
//# sourceMappingURL=BaseController.js.map                                                                                                                                                                                                                     